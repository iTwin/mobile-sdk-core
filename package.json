{
  "name": "@itwin/mobile-sdk-core",
  "version": "0.10.37",
  "description": "Bentley iTwin Mobile Core module",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/mobile-sdk-core"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/mobile-sdk-core.js",
  "types": "lib/mobile-sdk-core.d.ts",
  "files": [
    "lib/**/*",
    "!lib/**/*.map"
  ],
  "scripts": {
    "build": "npm run copy:assets && tsc 1>&2",
    "clean": "rimraf lib",
    "copy:assets": "cpx \"./src/**/*.{*css,svg}\" ./lib && cpx \"./public/**/*\" ./lib/public",
    "lint": "eslint -f visualstudio \"./src/**/*.ts\" 1>&2",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": "plugin:@itwin/itwinjs-recommended",
    "rules": {
      "nonblock-statement-body-position": "off"
    }
  },
  "dependencies": {
    "@itwin/core-bentley": "3.6.2",
    "@itwin/core-common": "3.6.2",
    "@itwin/core-frontend": "3.7.3",
    "@itwin/core-geometry": "3.6.2",
    "@itwin/core-i18n": "3.6.2",
    "@itwin/core-markup": "3.6.2",
    "@itwin/core-orbitgt": "3.6.2",
    "@itwin/core-quantity": "3.6.2",
    "@itwin/presentation-common": "3.6.2",
    "@itwin/presentation-frontend": "3.6.2",
    "@itwin/webgl-compatibility": "3.6.2",
    "base64-js": "^1.5.1"
  },
  "devDependencies": {
    "@itwin/eslint-plugin": "3.6.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cpx2": "^4.2.0",
    "eslint": "^7.31.0",
    "typescript": "~4.4.0"
  }
}
